{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                /* .cc、.h ファイルのinclude部分の Path 設定 */
                "${workspaceFolder}/**",
                "${workspaceFolder}/windows/flutter",
                "${workspaceFolder}/windows/flutter/ephemeral",
                "${workspaceFolder}/windows/flutter/ephemeral/.plugin_symlinks/cloud_firestore/windows/include",
                "${workspaceFolder}/windows/flutter/ephemeral/.plugin_symlinks/firebase_auth/windows/include",
                "${workspaceFolder}/windows/flutter/ephemeral/.plugin_symlinks/firebase_core/windows/include",
                "${workspaceFolder}/"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.22621.0",
            "compilerPath": "cl.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-msvc-x64"
        }
    ],
    "version": 4
}